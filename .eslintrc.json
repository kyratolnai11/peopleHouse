    "browser": true,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "eslint:recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "strict": 1
  },
  "root": true
}
